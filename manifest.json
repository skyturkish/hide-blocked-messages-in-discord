{
  "manifest_version": 3,
  "name": "Hide blocked discord messages",
  "version": "1.0",
  "description": "Hide blocked discord messages",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/hide.js"],
      "matches": ["https://discord.com/channels/*/*"]
    }
  ]
}
